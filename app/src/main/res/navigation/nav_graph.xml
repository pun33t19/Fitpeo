<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.fitpeodemoapp.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_passDetailsFragment"
            app:destination="@id/passDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/passDetailsFragment"
        android:name="com.example.fitpeodemoapp.PassDetailsFragment"
        android:label="fragment_pass_details"
        tools:layout="@layout/fragment_pass_details" />
    <fragment
        android:id="@+id/officeFragment"
        android:name="com.example.fitpeodemoapp.OfficeFragment"
        android:label="fragment_office"
        tools:layout="@layout/fragment_office" />
    <fragment
        android:id="@+id/peopleFragment"
        android:name="com.example.fitpeodemoapp.PeopleFragment"
        android:label="fragment_people"
        tools:layout="@layout/fragment_people" />
</navigation>